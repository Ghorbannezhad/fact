{
    "name": "Ghorbannezhad/fact",
    "description": "This is the fact generator package",
    "type": "metapackage",
    "license": "MIT",
    "authors": [
        {
            "name": "Faezeh Ghorbannezhad",
            "email": "fghorbannezhad@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.5.* || 5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Ghorbannezhad\\Fact\\": "src/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Ghorbannezhad\\Fact\\FactServiceProvider"
            ],
            "aliases": {
                "ReadMore": "Ghorbannezhad\\Fact\\FactFacade"
            }
        }
    }
}
